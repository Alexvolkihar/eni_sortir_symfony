<!DOCTYPE html>
<html lang="Fr">
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body class="h-screen text-gray-700">
		<header class="sticky top-0">
			{% include 'menu/nav.html.twig' %}
		</header>
		<main class="px-10 md:px-20 lg:px-40 mb-10">
			{% for message in app.flashes('success') %}
				<div class="text-green-400 border-green-400">
					{{ message }}
				</div>
			{% endfor %}
			{% for message in app.flashes('error') %}
				<div class="text-red-400 border-red-400">
					{{ message }}
				</div>
			{% endfor %}
			{% block body %}{% endblock %}
		</main>
	</body>
</html>
<!--Cc -->